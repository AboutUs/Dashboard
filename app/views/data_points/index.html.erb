<h1>Listing data_points</h1>

<table>
  <tr>
    <th>Family</th>
    <th>Name</th>
    <th>Value num</th>
    <th>Value str</th>
    <th>Recorded at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @data_points.each do |data_point| %>
  <tr>
    <td><%= data_point.family %></td>
    <td><%= data_point.name %></td>
    <td><%= data_point.value_num %></td>
    <td><%= data_point.value_str %></td>
    <td><%= data_point.recorded_at %></td>
    <td><%= link_to 'Show', data_point %></td>
    <td><%= link_to 'Edit', edit_data_point_path(data_point) %></td>
    <td><%= link_to 'Destroy', data_point, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Data point', new_data_point_path %>
